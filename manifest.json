{
    "manifest_version": 2,
    "name": "hublr",
    "description": "A Chrome extension for nerds",
    "homepage_url": "https://github.com/ali/hublr",
    "version": "0.0.3",
    "author": [
        {"name": "Alisha Ukani"},
        {"name": "Ali Ukani <ali.ukani@gmail.com>"}
    ],
    "permissions": ["*://github.com/*"],
    "background": {
        "scripts": ["build/eventPage.bundle.js"],
        "persistent": false
    },
    "content_scripts": [
        {
            "matches": ["*://github.com/*"],
            "js": ["build/index.bundle.js"]
        }
    ]
}
