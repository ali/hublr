{
  "manifest_version": 2,
  "name": "hublr",
  "description": "A Chrome extension for nerds",
  "homepage_url": "https://github.com/ali/hublr",
  "version": "0.0.1",
  "author": [
    {
      "name": "Alisha Ukani"
    },
    {
      "name": "Ali Ukani",
      "email": "ali.ukani@gmail.com"
    }
    ],
  "permissions": [
    "storage",
    "tabs",
    "https://github.com/",
    "http://github.com/"
    ],
  "content_scripts": [
    {
      "matches": [
        "*://github.com/"
        ],
      "js": [
        "vendor/zepto.min.js",
        "hublr.js"
        ],
      "css": ["hublr.css"]
    }
  ],
  "web_accessible_resources": [
    "hublr.map",
    "hublr.coffee"
  ]
}

